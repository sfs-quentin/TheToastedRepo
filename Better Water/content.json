		"Fill bridge holes": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		},
		"Witch Swamp": {
		"AllowValues": "Green, Blue, Brown, Disabled",
		"Default": "Green",
        "Section": "Extra",
		},
		"Mutant Bug Lair": {
		"AllowValues": "Dark, Blue, Disabled",
		"Default": "Dark",
        "Section": "Extra",
		},
		"Desert": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		},
		"Fish pond": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		},
		"Caves mode": {
		"AllowValues": "Default, Seasonal, Dirt",
		"Default": "Default",
        "Section": "Advanced",
