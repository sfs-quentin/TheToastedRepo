	"DynamicTokens": [
// Dynamic token to change water color animation on or outside island under a single logic
	// Water color when on the island 
		{
		"Name": "watercolors",
		"Value": "{{Color Island}}",
		"When": {
			"Color Island": "Island, Teal, Blue, Bright, Dark",
			"LocationContext": "Island",
			},
		},
	// Water color normally
		{
		"Name": "watercolors",
		"Value": "{{Color}}",
		"When": {"Color": "Teal, Blue, Bright, Dark"},
		},
// Dynamic token to change map tiles transparency with Mixed option
	// Use the selected option if it's Translucent, Semi-Opaque or Opaque
		// Translucent when Mixed option selected
		{
		"Name": "Transparencies_Mixed_Translucent",
		"Value": "{{Map Tiles Transparency}}",
		"When": {"Map Tiles Transparency": "Translucent, Semi-Opaque, Opaque"},
		},
		{
		"Name": "Transparencies_Mixed_Translucent",
		"Value": "Translucent",
		"When": {"Map Tiles Transparency": "Mixed"},
		},
		// Semi-Opaque when Mixed option selected
		{
		"Name": "Transparencies_Mixed_SemiOpaque",
		"Value": "{{Map Tiles Transparency}}",
		"When": {"Map Tiles Transparency": "Translucent, Semi-Opaque, Opaque"},
		},
		{
		"Name": "Transparencies_Mixed_SemiOpaque",
		"Value": "Semi-Opaque",
		"When": {"Map Tiles Transparency": "Mixed"},
		},
// Dynamic token to change caves under a single logic
	// Default: uses Map Tiles Transparency
		// the same for options Mixed, Translucent and Semi-Opaque
		{
		"Name": "CaveMode",
		"Value": "mixed",
		"When": {
			"Caves": true,
			"Map Tiles Transparency": "Mixed, Translucent, Semi-Opaque",
			"Caves mode": "Default",
			},
		},
		// opaque
		{
		"Name": "CaveMode",
		"Value": "opaque",
		"When": {
			"Caves": true,
			"Map Tiles Transparency": "Opaque",
			"Caves mode": "Default",
			},
		},
	// Dirt
		{
		"Name": "CaveMode",
		"Value": "Dirt",
		"When": {
			"Caves": true,
			"Caves mode": "Dirt"},
		},
	// Seasonal
		{
		"Name": "CaveMode",
		"Value": "{{season}}",
		"When": {
			"Caves": true,
			"Caves mode": "Seasonal"},
		},
	],
